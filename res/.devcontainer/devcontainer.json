{
    "name": "remotend",
    "build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/vscode/remotend,type=bind,consistency=cache",
    "workspaceFolder": "/home/vscode/remotend",
    "postStartCommand": ".devcontainer/build.sh",
    "features": {
        "ghcr.io/devcontainers/features/java:1": {},
        "ghcr.io/akhildevelops/devcontainer-features/android-cli:latest": {
            "PACKAGES": "platform-tools,ndk;23.2.8568313"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vadimcn.vscode-lldb",
                "mutantdino.resourcemonitor",
                "rust-lang.rust-analyzer",
                "tamasfe.even-better-toml",
                "serayuzgur.crates",
                "mhutchie.git-graph",
                "eamodio.gitlens"
            ],
            "settings": {
                "files.watcherExclude": {
                    "**/target/**": true
                }
            }
        }
    }
}
